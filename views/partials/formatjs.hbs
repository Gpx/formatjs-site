{{#if usesReactIntl}}
<script src="/vendor/react-intl/react-intl{{min}}.js"></script>
<script src="/vendor/formatjs/react-intl-locale-data.js"></script>
{{/if}}

{{#if usesDustIntl}}
<script src="/vendor/dust/dust-full{{min}}.js"></script>
<script src="/vendor/dust-intl/dust-intl{{min}}.js"></script>
<script src="/vendor/formatjs/dust-intl-locale-data.js"></script>
<script>
    DustIntl.registerWith(dust);
</script>
{{/if}}

{{#if usesHandlebarsIntl}}
<script src="/vendor/handlebars/handlebars{{min}}.js"></script>
<script src="/vendor/handlebars-intl/handlebars-intl{{min}}.js"></script>
<script src="/vendor/formatjs/handlebars-intl-locale-data.js"></script>
<script>
    HandlebarsIntl.registerWith(Handlebars);
</script>
{{/if}}

{{#if usesEmberIntl}}
<script src="/vendor/jquery/jquery{{min}}.js"></script>
<script src="/vendor/ember/ember{{#if min}}{{min}}{{else}}.debug{{/if}}.js"></script>
<script src="/vendor/ember/ember-template-compiler.js"></script>
<script src="/vendor/ember-intl/ember-intl{{min}}.js"></script>
<script src="/vendor/formatjs/ember-intl-locale-data.js"></script>
{{/if}}
